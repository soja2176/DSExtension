{
  "name": "Full Screen Start+ y Disney+",
  "version": "1",
  "manifest_version": 3,
  "description": "Extension to watch Disney+ in full screen with Start+ automatically",
  "permissions": ["tabs", "fullscreen"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icon.png"
  },
  "host_permissions": [
    "https://www.startplus.com/*",
    "https://www.disneyplus.com/*"
  ]
}
